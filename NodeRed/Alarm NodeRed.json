        "id": "66b12c58.b2c1f4",
        "type": "function",
        "z": "a520e472.c45e18",
        "name": "If Triggered message",
        "func": "var dt = new Date();    // get the current date and time\n\n// edge detect a state change\nif( context.get(\"lastState\") == msg.payload)\n    return null;\n    \ncontext.set(\"lastState\", msg.payload);\n\n// if a TRIGGERED event\nif( msg.payload == \"TR\")\n{\n    msg.payload = \"Triggered at: \" + dt.toDateString() + \" \" + dt.toTimeString();\n    return msg;\n}\n\nif( msg.payload == \"ARM\")\n{\n    msg.payload = \"Armed at: \" + dt.toDateString() + \" \" + dt.toTimeString();\n    return msg;\n}\n\nif( msg.payload == \"DIS\")\n{\n    msg.payload = \"Disarmed at: \" + dt.toDateString() + \" \" + dt.toTimeString();\n    return msg;\n}\n\n\nreturn null;",
        "outputs": 1,
        "noerr": 0,
