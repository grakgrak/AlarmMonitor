    {
        "id": "13e02b39.e16175",
        "type": "function",
        "z": "a520e472.c45e18",
        "name": "Heartbeat message",
        "func": "if( msg.payload == \"NO ALARM\")\n{\n    context.set(\"IsDown\", true);    // flag the alarm is down\n\n    var dt = new Date();    // get the current date and time\n\n    var text = \"No Heartbeat seen at: \" + dt.toDateString() + \" \" + dt.toTimeString();\n\n    msg.payload = text;\n\n    return msg;\n}\n\nvar isDown = context.get(\"IsDown\") || false;\n\nif( isDown )\n{\n    context.set(\"IsDown\", false);    // flag the alarm is back\n\n    var dt = new Date();    // get the current date and time\n\n    var text = \"Heartbeat back at: \" + dt.toDateString() + \" \" + dt.toTimeString();\n\n    msg.payload = text;\n    \n    return msg;\n}\n\nreturn null;",
        "outputs": 1,
        "noerr": 0,
        "x": 570,
        "y": 160,
        "wires": [
